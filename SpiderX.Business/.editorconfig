[*.cs]

# CA1305: 指定 IFormatProvider
dotnet_diagnostic.CA1305.severity = silent

# CA1031: 不捕获常规异常类型
dotnet_diagnostic.CA1031.severity = suggestion

# CA1307: 指定 StringComparison
dotnet_diagnostic.CA1307.severity = silent

# CA2000: 丢失范围之前释放对象
dotnet_diagnostic.CA2000.severity = silent

# CA2007: 考虑对等待的任务调用 ConfigureAwait
dotnet_diagnostic.CA2007.severity = suggestion

# IDE0058: 永远不会使用表达式值
csharp_style_unused_value_expression_statement_preference = discard_variable:suggestion
